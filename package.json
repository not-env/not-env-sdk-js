{
  "name": "not-env-sdk",
  "version": "0.1.1",
  "description": "JavaScript/TypeScript SDK for not-env environment variable management",
  "main": "dist/register.js",
  "types": "dist/register.d.ts",
  "exports": {
    ".": {
      "require": "./dist/register.js",
      "import": "./dist/register.js",
      "types": "./dist/register.d.ts"
    },
    "./register": {
      "require": "./dist/register.js",
      "import": "./dist/register.js",
      "types": "./dist/register.d.ts"
    }
  },
  "author": "Gavin Johnson <gavinj1984@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/not-env/not-env-sdk-js.git"
  },
  "homepage": "https://github.com/not-env/not-env-sdk-js#readme",
  "bugs": {
    "url": "https://github.com/not-env/not-env-sdk-js/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "environment",
    "variables",
    "config",
    "not-env"
  ],
  "scripts": {
    "build": "tsc",
    "test": "NODE_OPTIONS='--no-webstorage' jest",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.12.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
